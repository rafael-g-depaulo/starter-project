{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/strapi/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "build"
      },
      "outputs": [
        "apps/strapi/dist"
      ],
      "configurations": {
        "production": {
          "script": "build"
        }
      },
      "dependsOn": [
        "install"
      ]
    },
    "serve": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "develop"
      },
      "configurations": {
        "production": {
          "script": "start"
        }
      },
      "dependsOn": [
        "install"
      ]
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn install",
        "cwd": "apps/strapi"
      }
    },
    "generate-types": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "generate-types"
      },
      "outputs": [
        "apps/strapi/schemas.d.ts"
      ]
    }
  },
  "tags": []
}
